let daysOfMonths = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31]

print(daysOfMonths, "\n")


let months = ["january", "february", "march", "april", "may", "june", "jule", "august", "september", "october", "november", "december"]

var a = 0

for a in 0..<months.count {
    print("\(months[a]) - \(daysOfMonths[a])")
}
